/*CALLOUT BOXES*/
.callout {
    margin: 20px 0;
    padding: 20px;
    border-left: 3px solid #eee;
}

.callout h4, h5 {
    margin-top: 0;
    margin-bottom: 5px;
    font-size: 17px;
}

.callout-success {
    background-color: #D8E6B0;
    border-color: #95C11E;
}

.callout-success h4,
.callout-success h5,
.callout-success strong {
    color: #000;
}

.icon-attention_positive {
    background-image: url('../../../img/icons/icon-attention_positive.png');
    display: inline-block;
}

.callout-info {
    background-color: #CBD6E2;
    border-color: #5D7289;
}

.callout-info h4,
.callout-info h5,
.callout-info strong {
    color: #1E3A59;
}

.callout-warning {
    background-color: #FFE9B7;
    border-color: #FFB611;
}

.callout-warning h4,
.callout-warning h5,
.callout-warning strong {
    color: #000;
}

.callout-danger {
    background-color: #E4A9A6;
    border-color: #BE1621;
}

.callout-danger h4,
.callout-danger h5,
.callout-danger strong {
    color: #000;
}

.icon-attention_negative {
    background-image: url('../../../img/icons/icon-attention_negative.png');
    display: inline-block;
}

#footer .caption {
    color: #E5E6E8;
    font-size: .8em;
}

/* quotes,fancy quotes and citations */
blockquote {
    /* over-ride the bootstrap blockquoute styling */
    border: none !important;
    font-weight: normal;
    font-size: inherit;
}

blockquote.mark {
    quotes: "\201C" "\201D";
}

blockquote.mark {
    font-style: italic;
    display: inline-block;
    padding-top: .9em;
    padding-left: 2.2em;
    padding-right: 2.2em;
    padding-bottom: .0em;
    background: url(../../../img/quote-open.png) top left no-repeat, url(../../../img/quote-close.png) bottom right no-repeat;
}

cite {
    color: #999;
    font-size: 0.6em;
    display: block;
    margin-top: 0.1em;
    text-align: right;
}

blockquote.mark + cite:before {
    content: "\2014 \2009";
}

.nav a {
    text-decoration: none;
}

.dit-spacer {
    background-color: #D3D7E7;
    font-size: 10px;
}

.dit-spacer.invisible {
    visibility: hidden;
}

