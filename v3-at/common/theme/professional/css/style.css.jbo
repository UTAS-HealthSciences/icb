#container {
    margin-top: 44px;
    margin-bottom: 36px;
    -webkit-box-shadow: 0px 0px 140px 80px rgba(255, 255, 255, .30);
    -moz-box-shadow: 0px 0px 140px 80px rgba(255, 255, 255, .30);
    box-shadow: 0px 0px 140px 80px rgba(255, 255, 255, .30);
}

#header {
    color: white;
    text-align: left;

    background: -webkit-linear-gradient(60deg, #616363 55%, #666868 45%); /* For Safari 5.1 to 6.0 */
    background: -o-linear-gradient(60deg, #616363 55%, #666868 45%); /* For Opera 11.1 to 12.0 */
    background: -moz-linear-gradient(60deg, #616363 55%, #666868 45%); /* For Firefox 3.6 to 15 */
    background: linear-gradient(60deg, #616363 55%, #666868 45%); /* Standard syntax (must be last) */
}

a {
    text-decoration: underline;
}

a.pdf-link {
    display: none;
}

#header img {
    margin-top: 1em;
    margin-bottom: 1em;
}

#header .text-right img {
    margin-right: 1em;
}

.utas-logo {
    /* background-color:#C40000;*/
    padding: 1em; /*margin:1em;*/
    margin: 1em;
}

h1,
.h1 {
    font-size: 28px;
}
h2,
.h2 {
    font-size: 23px;
}
h3,
.h3 {
    font-size: 18px;
}
h4,
.h4 {
    font-size: 14px;
}
h5,
.h5 {
    font-size: 11px;
}
h6,
.h6 {
    font-size: 9px;
}

#header h1 {
    font-family: "omnes-pro", sans-serif;
    /*margin-top: .2em;*/
    /*margin-bottom: 0.2em;*/
    font-size: 2em;
    font-weight: 300;
}

#header h2 {
    font-family: "proxima-nova", sans-serif;
